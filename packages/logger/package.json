{
  "name": "@gqty/logger",
  "version": "1.0.4",
  "homepage": "https://gqty.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/PabloSzx/gqty.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "bob-esbuild build",
    "dev": "bob-esbuild watch",
    "prepare": "bob-esbuild build",
    "postpublish": "gh-release",
    "size": "size-limit",
    "start": "bob-esbuild watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "prettier": "^2.4.0",
    "serialize-error": "^8.1.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/prettier": "^2.3.2",
    "bob-esbuild-cli": "^1.0.1",
    "gqty": "workspace:^1.1.3",
    "jest": "^27.1.1",
    "rimraf": "^3.0.2",
    "size-limit": "^5.0.3",
    "test-utils": "workspace:^0.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "peerDependencies": {
    "gqty": "workspace:^1.1.2"
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "size-limit": [
    {
      "path": "dist/gqty-logger.cjs.production.min.js",
      "limit": "260 KB"
    },
    {
      "path": "dist/gqty-logger.esm.js",
      "limit": "260 KB"
    }
  ]
}
