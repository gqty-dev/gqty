{
  "name": "@gqty/logger",
  "version": "4.0.0",
  "homepage": "https://gqty.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/gqty-dev/gqty.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "maintainers": [
    "Vicary Archangel <vicary.archangel@member.mensa.org>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs",
      "types": "./dist/*.d.ts"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "bob-esbuild build",
    "dev": "bob-esbuild watch",
    "prepare": "bob-esbuild build",
    "postpublish": "gh-release",
    "size": "size-limit",
    "start": "bob-esbuild watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "prettier": "^3.0.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "@types/node": "^20.14.5",
    "bob-esbuild-cli": "^4.0.0",
    "esbuild": "^0.21.5",
    "gqty": "workspace:^",
    "jest": "^29.7.0",
    "size-limit": "^11.1.4",
    "test-utils": "workspace:^",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "gqty": "workspace:^"
  },
  "engines": {
    "node": "^12.20.0 || >=14.13.0"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "size-limit": [
    {
      "path": "dist/gqty-logger.cjs.production.min.js",
      "limit": "260 KB"
    },
    {
      "path": "dist/gqty-logger.esm.js",
      "limit": "260 KB"
    }
  ]
}
