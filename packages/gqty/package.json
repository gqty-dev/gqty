{
  "name": "gqty",
  "version": "3.1.0",
  "description": "gqty client without queries",
  "homepage": "https://gqty.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/gqty-dev/gqty.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "maintainers": [
    "Vicary Archangel <vicary.archangel@member.mensa.org>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs",
      "types": "./dist/*.d.ts"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "bob-esbuild build",
    "dev": "bob-esbuild watch",
    "prepare": "bob-esbuild build",
    "postpublish": "gh-release",
    "size": "size-limit",
    "start": "bob-esbuild watch",
    "test": "jest",
    "test:specific": "jest test/interfaces-unions.test.ts --watch --no-coverage -u",
    "test:watch": "jest --watch",
    "test:watch-coverage": "rimraf coverage && mkdir -p coverage/lcov-report && concurrently --raw \"jest --watchAll\" \"serve -l 8787 coverage/lcov-report\" \"wait-on tcp:8787 coverage/lcov-report/index.html && open-cli http://localhost:8787\""
  },
  "dependencies": {
    "debounce-microtasks": "^0.1.7",
    "flatted": "^3.3.1",
    "frail-map": "^1.0.10",
    "just-extend": "^6.2.0",
    "just-has": "^2.3.0",
    "just-memoize": "^2.2.0",
    "just-safe-get": "^4.2.0",
    "just-safe-set": "^4.2.1",
    "multidict": "^1.0.9",
    "object-hash": "^3.0.0",
    "p-defer": "^3.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "@types/node": "^20.14.5",
    "@types/object-hash": "^3.0.6",
    "@types/wait-on": "^5.3.4",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "bob-esbuild-cli": "^4.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.14.0",
    "graphql": "^16.8.2",
    "graphql-sse": "^2.5.3",
    "graphql-ws": "^5.16.0",
    "jest": "^30.0.0-alpha.5",
    "just-memoize": "^2.2.0",
    "open-cli": "^8.0.0",
    "p-lazy": "^3.1.0",
    "rimraf": "^5.0.7",
    "serve": "^14.2.3",
    "size-limit": "^11.1.4",
    "test-utils": "workspace:^0.1.0",
    "tsc-watch": "^6.2.0",
    "type-fest": "^4.20.1",
    "typescript": "^5.4.5",
    "wait-on": "^7.2.0",
    "ws": "^8.17.1"
  },
  "peerDependencies": {
    "graphql": "*",
    "graphql-sse": "^2.5.3",
    "graphql-ws": "^5.16.0"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    },
    "graphql-sse": {
      "optional": true
    },
    "graphql-ws": {
      "optional": true
    }
  },
  "engines": {
    "node": "^12.20.0 || >=14.13.0"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:eslint-plugin-you-dont-need-lodash-underscore/compatible"
    ],
    "ignorePatterns": [
      "node_modules/**",
      "**/*.js",
      "**/*.test.ts"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "@typescript-eslint/no-import-type-side-effects": "error",
      "@typescript-eslint/consistent-type-imports": [
        "error",
        {
          "fixStyle": "inline-type-imports"
        }
      ]
    }
  },
  "publishConfig": {
    "directory": "dist"
  },
  "size-limit": [
    {
      "path": "dist/gqty.cjs.production.min.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/gqty.esm.js",
      "limit": "20 KB"
    }
  ]
}
