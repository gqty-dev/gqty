{
  "name": "gqty",
  "version": "2.3.0",
  "description": "gqty client without queries",
  "homepage": "https://gqty.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/gqty-dev/gqty.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "bob-esbuild build",
    "dev": "bob-esbuild watch",
    "playground": "jest test/playground.test.ts --watch --no-coverage",
    "prepare": "bob-esbuild build",
    "postpublish": "gh-release",
    "size": "size-limit",
    "start": "bob-esbuild watch",
    "test": "jest",
    "test:specific": "jest test/interfaces-unions.test.ts --watch --no-coverage -u",
    "test:watch": "jest --watch",
    "test:watch-coverage": "rimraf coverage && mkdirp coverage/lcov-report && concurrently --raw \"jest --watchAll\" \"serve -l 8787 coverage/lcov-report\" \"wait-on tcp:8787 coverage/lcov-report/index.html && open-cli http://localhost:8787\""
  },
  "dependencies": {
    "@gqty/utils": "workspace:^1.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.2",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^18.13.0",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^18.0.10",
    "@types/wait-on": "^5.3.1",
    "@types/ws": "^8.5.4",
    "bob-esbuild-cli": "^4.0.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.6",
    "graphql": "^16.6.0",
    "jest": "^29.4.2",
    "mkdirp": "^2.1.3",
    "open-cli": "^7.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^4.1.2",
    "serve": "^14.2.0",
    "size-limit": "^8.1.2",
    "test-utils": "workspace:^0.1.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5",
    "wait-on": "^7.0.1",
    "ws": "^8.12.0"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    }
  },
  "engines": {
    "node": "^12.20.0 || >=14.13.0"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "size-limit": [
    {
      "path": "dist/gqty.cjs.production.min.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/gqty.esm.js",
      "limit": "20 KB"
    }
  ]
}
