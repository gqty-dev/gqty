{
  "name": "gqty",
  "version": "1.0.3",
  "description": "gqty client without queries",
  "homepage": "https://gqty.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/PabloSzx/gqty.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "bob-esbuild build",
    "dev": "nodemon",
    "dev:nodemon": "tsup src -d dist && node dist/QueryBuilder/buildQuery.js",
    "playground": "jest test/playground.test.ts --watch --no-coverage",
    "prepare": "bob-esbuild build",
    "postpublish": "gh-release",
    "size": "size-limit",
    "start": "bob-esbuild watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch-coverage": "rimraf coverage && mkdirp coverage/lcov-report && concurrently --raw \"jest --watchAll\" \"serve -l 8787 coverage/lcov-report\" \"wait-on tcp:8787 coverage/lcov-report/index.html && open-cli http://localhost:8787\""
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.5",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/concurrently": "^6.2.1",
    "@types/lodash": "^4.14.172",
    "@types/lodash-es": "^4.17.4",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.6.1",
    "@types/nodemon": "^1.19.1",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/rimraf": "^3.0.1",
    "@types/tsc-watch": "^4.2.0",
    "@types/wait-on": "^5.3.1",
    "@types/ws": "^7.4.7",
    "bob-esbuild-cli": "^1.0.1",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "get-port": "^5.1.1",
    "graphql": "^15.5.1",
    "jest": "^27.0.6",
    "mercurius": "^8.1.2",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.12",
    "open-cli": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "serve": "^12.0.0",
    "size-limit": "^5.0.3",
    "test-utils": "workspace:^0.1.0",
    "tsc-watch": "^4.4.0",
    "tslib": "^2.3.1",
    "tsup": "^4.13.1",
    "typescript": "^4.4.1-rc",
    "wait-on": "^6.0.0",
    "ws": "^7.5.3"
  },
  "peerDependencies": {
    "graphql": "^15.5.1"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    }
  },
  "publishConfig": {
    "directory": "dist"
  },
  "size-limit": [
    {
      "path": "dist/gqty.cjs.production.min.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/gqty.esm.js",
      "limit": "20 KB"
    }
  ]
}
