{
  "name": "@gqty/react",
  "version": "2.1.0",
  "homepage": "https://gqty.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/gqty-dev/gqty.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "bob-esbuild build",
    "dev": "bob-esbuild watch",
    "prepare": "bob-esbuild build",
    "postpublish": "gh-release",
    "size": "size-limit",
    "start": "bob-esbuild watch",
    "start:with:example": "bob-esbuild watch --onSuccess \"pnpm -r --filter react-example dev\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "react-ssr-prepass": "^1.5.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.35",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "bob-esbuild-cli": "^4.0.0",
    "esbuild": "^0.14.39",
    "gqty": "workspace:^2.2.0",
    "graphql": "^16.5.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "size-limit": "^7.0.8",
    "test-utils": "workspace:^0.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "gqty": "workspace:^2.0.4",
    "graphql": "*",
    "react": ">=16.14.0"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": "^12.20.0 || >=14.13.0"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "size-limit": [
    {
      "path": "dist/gqty-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gqty-react.esm.js",
      "limit": "10 KB"
    }
  ]
}
