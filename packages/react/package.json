{
  "name": "@gqty/react",
  "version": "3.1.1",
  "description": "The No-GraphQL Client for React",
  "keywords": [
    "cache",
    "client",
    "gqty",
    "graphql",
    "hooks",
    "react",
    "reactive",
    "state"
  ],
  "homepage": "https://gqty.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/gqty-dev/gqty.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "maintainers": [
    "Vicary Archangel <vicary.archangel@member.mensa.org>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "bob-esbuild build",
    "dev": "bob-esbuild watch",
    "lint": "eslint src/**/*.{ts,tsx}",
    "prepare": "bob-esbuild build",
    "postpublish": "gh-release",
    "start": "bob-esbuild watch",
    "start:with:example": "bob-esbuild watch --onSuccess \"pnpm -r --filter react-example dev\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:eslint-plugin-you-dont-need-lodash-underscore/compatible"
    ],
    "rules": {
      "@typescript-eslint/consistent-type-imports": [
        "error",
        {
          "fixStyle": "inline-type-imports"
        }
      ],
      "@typescript-eslint/no-import-type-side-effects": "error"
    },
    "ignorePatterns": [
      "node_modules/**",
      "**/*.js",
      "**/*.test.ts"
    ]
  },
  "dependencies": {
    "@react-hookz/web": "^25.2.0",
    "multidict": "^1.0.9",
    "p-debounce": "^4.0.0",
    "p-defer": "^3.0.0",
    "react-ssr-prepass": "^1.6.0",
    "use-sync-external-store": "^1.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/eslint": "^8.56.12",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.0",
    "@types/react": "^18.3.23",
    "@types/use-sync-external-store": "^0.0.6",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.31.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "^6.14.0",
    "gqty": "workspace:^",
    "graphql": "^16.11.0",
    "graphql-sse": "^2.5.4",
    "graphql-ws": "^5.16.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lodash-es": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-test-renderer": "^18.3.1",
    "test-utils": "workspace:^",
    "type-fest": "^4.41.0",
    "typescript-eslint": "^8.38.0"
  },
  "peerDependencies": {
    "gqty": "workspace:^3.5.0",
    "graphql": "*",
    "graphql-sse": "^2.5.4",
    "graphql-ws": "^5.16.2",
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "graphql-sse": {
      "optional": true
    },
    "graphql-ws": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16 <=25"
  },
  "publishConfig": {
    "directory": "dist"
  }
}
