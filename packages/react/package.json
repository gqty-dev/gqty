{
  "name": "@gqty/react",
  "version": "2.0.0",
  "homepage": "https://gqty.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/gqty-dev/gqty.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "bob-esbuild build",
    "dev": "bob-esbuild watch",
    "prepare": "bob-esbuild build",
    "postpublish": "gh-release",
    "size": "size-limit",
    "start": "bob-esbuild watch",
    "start:with:example": "bob-esbuild watch --onSuccess \"pnpm -r --filter react-example dev\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "react-ssr-prepass": "^1.4.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/react": "^17.0.31",
    "@types/react-dom": "^17.0.10",
    "bob-esbuild-cli": "^2.0.0",
    "gqty": "workspace:^2.0.3",
    "graphql": "^15.6.1",
    "jest": "^27.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "size-limit": "^6.0.3",
    "test-utils": "workspace:^0.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "gqty": "workspace:^2.0.0",
    "graphql": "*",
    "react": ">=16.8"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    },
    "react": {
      "optional": true
    }
  },
  "engines": {
    "node": "^12.20.0 || >=14.13.0"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "size-limit": [
    {
      "path": "dist/gqty-react.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gqty-react.esm.js",
      "limit": "10 KB"
    }
  ]
}
