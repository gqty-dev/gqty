{
  "name": "@gqty/cli",
  "version": "4.1.0",
  "description": "Interactive codegen for GQty",
  "keywords": [
    "client",
    "codegen",
    "gqty",
    "graphql",
    "interactive",
    "reactive"
  ],
  "homepage": "https://gqty.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/gqty-dev/gqty.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "maintainers": [
    "Vicary Archangel <vicary.archangel@member.mensa.org>"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "gqty": "bin/gqty.mjs"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "bob-tsm build.ts",
    "prepare": "bob-tsm build.ts",
    "postpublish": "gh-release",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch-coverage": "pnpm dlx rimraf coverage && mkdir -p coverage/lcov-report && concurrently --raw \"jest --watchAll\" \"serve -l 8787 coverage/lcov-report\" \"wait-on tcp:8787 coverage/lcov-report/index.html && open-cli http://localhost:8787\""
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/consistent-type-imports": [
        "error",
        {
          "fixStyle": "inline-type-imports"
        }
      ],
      "@typescript-eslint/no-import-type-side-effects": "error"
    },
    "ignorePatterns": [
      "node_modules/**",
      "**/*.js",
      "**/*.test.ts"
    ]
  },
  "dependencies": {
    "@graphql-codegen/core": "^4.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@graphql-tools/delegate": "^10.0.15",
    "@graphql-tools/utils": "^10.3.2",
    "@graphql-tools/wrap": "^10.0.5",
    "@inquirer/prompts": "^5.3.2",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "cosmiconfig": "^9.0.0",
    "cross-fetch": "^4.0.0",
    "fast-glob": "^3.3.2",
    "gqty": "workspace:^",
    "graphql": "^16.9.0",
    "lodash-es": "^4.17.21",
    "micromatch": "^4.0.7",
    "prettier": "^2.8.8"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^20.14.11",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "bob-tsm": "^1.1.2",
    "esbuild": "^0.23.0",
    "eslint": "^8.57.0",
    "open-cli": "^8.0.0",
    "p-lazy": "^3.1.0",
    "serve": "^14.2.3",
    "size-limit": "^11.1.4",
    "test-utils": "workspace:^",
    "tmp-promise": "^3.0.3",
    "typescript": "^5.5.4",
    "wait-on": "^7.2.0"
  },
  "optionalDependencies": {
    "trading-signals": "^5.0.4"
  },
  "publishConfig": {
    "directory": "dist"
  }
}
