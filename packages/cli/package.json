{
  "name": "@gqty/cli",
  "version": "3.2.0",
  "homepage": "https://gqty.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/gqty-dev/gqty.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./deps": "./dist/deps.js",
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "bin": {
    "gqty": "bin/gqty.mjs"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "bob-tsm build.ts",
    "prepare": "bob-tsm build.ts",
    "postpublish": "gh-release",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch-coverage": "rimraf coverage && mkdirp coverage/lcov-report && concurrently --raw \"jest --watchAll\" \"serve -l 8787 coverage/lcov-report\" \"wait-on tcp:8787 coverage/lcov-report/index.html && open-cli http://localhost:8787\""
  },
  "dependencies": {
    "gqty": "workspace:^2.2.0",
    "undici": "^5.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/core": "^2.5.1",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-tools/delegate": "^8.7.8",
    "@graphql-tools/utils": "^8.6.10",
    "@graphql-tools/wrap": "^8.4.17",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/lodash": "^4.14.182",
    "@types/lodash.sortby": "^4.7.7",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^17.0.35",
    "bob-ts": "^4.0.0",
    "bob-tsm": "^1.0.0",
    "changesets-github-release": "^0.1.0",
    "commander": "^9.2.0",
    "cosmiconfig": "^7.0.1",
    "esbuild": "^0.14.39",
    "fast-glob": "^3.2.11",
    "graphql": "^16.5.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "open-cli": "^7.0.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "size-limit": "^7.0.8",
    "test-utils": "workspace:^0.1.0",
    "tmp-promise": "^3.0.3",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "wait-on": "^6.0.1"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    }
  },
  "engines": {
    "node": "^12.20.0 || >=14.13.0"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*",
        "dist/*/index.d.ts"
      ]
    }
  }
}
