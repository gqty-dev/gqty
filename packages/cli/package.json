{
  "name": "@gqty/cli",
  "version": "1.0.1",
  "homepage": "https://gqty.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/PabloSzx/gqty.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "bin": {
    "gqty": "bin/gqty.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "bob-esbuild build",
    "prepare": "bob-esbuild build",
    "postpublish": "gh-release",
    "size": "size-limit",
    "start": "bob-esbuild watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch-coverage": "rimraf coverage && mkdirp coverage/lcov-report && concurrently --raw \"jest --watchAll\" \"serve -l 8787 coverage/lcov-report\" \"wait-on tcp:8787 coverage/lcov-report/index.html && open-cli http://localhost:8787\""
  },
  "dependencies": {
    "@graphql-codegen/core": "^1.17.10",
    "@graphql-codegen/typescript": "^1.23.0",
    "@graphql-tools/delegate": "^8.0.3",
    "@graphql-tools/utils": "^8.0.1",
    "@graphql-tools/wrap": "^8.0.3",
    "commander": "^8.1.0",
    "cosmiconfig": "^7.0.0",
    "cross-fetch": "^3.1.4",
    "gqty": "workspace:^1.0.1",
    "mkdirp": "^1.0.4",
    "prettier": "^2.3.2"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.2",
    "@types/mkdirp": "^1.0.2",
    "bob-esbuild-cli": "^0.1.27",
    "changesets-github-release": "^0.0.3",
    "graphql": "^15.5.1",
    "open-cli": "^6.0.1",
    "rimraf": "^3.0.2",
    "serve": "^12.0.0",
    "size-limit": "^5.0.2",
    "test-utils": "workspace:^0.1.0",
    "tmp-promise": "^3.0.2",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5",
    "wait-on": "^6.0.0"
  },
  "peerDependencies": {
    "graphql": "^15.5.1"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "buildConfig": {
    "copy": [
      "bin"
    ]
  },
  "size-limit": [
    {
      "path": "dist/gqty-cli.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/gqty-cli.esm.js",
      "limit": "10 KB"
    }
  ]
}
