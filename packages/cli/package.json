{
  "name": "@gqty/cli",
  "version": "3.3.0",
  "homepage": "https://gqty.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/gqty-dev/gqty.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "maintainers": [
    "Vicary Archangel <vicary.archangel@member.mensa.org>"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./deps": "./dist/deps.js",
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs",
      "types": "./dist/*.d.ts"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "bin": {
    "gqty": "bin/gqty.mjs"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "bob-tsm build.ts",
    "prepare": "bob-tsm build.ts",
    "postpublish": "gh-release",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch-coverage": "rimraf coverage && mkdir -p coverage/lcov-report && concurrently --raw \"jest --watchAll\" \"serve -l 8787 coverage/lcov-report\" \"wait-on tcp:8787 coverage/lcov-report/index.html && open-cli http://localhost:8787\""
  },
  "dependencies": {
    "@graphql-codegen/core": "^4.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-tools/delegate": "^10.0.9",
    "@graphql-tools/utils": "^10.2.0",
    "@graphql-tools/wrap": "^10.0.5",
    "@inquirer/prompts": "^5.0.2",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cosmiconfig": "^9.0.0",
    "cross-fetch": "^4.0.0",
    "fast-glob": "^3.3.2",
    "gqty": "workspace:^",
    "graphql": "^16.8.1",
    "lodash-es": "^4.17.21",
    "micromatch": "^4.0.5",
    "prettier": "^2.8.8",
    "trading-signals": "^5.0.1"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/micromatch": "^4.0.7",
    "@types/node": "^20.12.8",
    "bob-tsm": "^1.1.2",
    "esbuild": "^0.20.2",
    "open-cli": "^8.0.0",
    "p-lazy": "^3.1.0",
    "rimraf": "^5.0.5",
    "serve": "^14.2.3",
    "size-limit": "^11.1.2",
    "test-utils": "workspace:^",
    "tmp-promise": "^3.0.3",
    "wait-on": "^7.2.0"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "typesVersions": {
    "*": {
      "dist/index.d.ts": [
        "dist/index.d.ts"
      ],
      "*": [
        "dist/*",
        "dist/*/index.d.ts"
      ]
    }
  }
}
