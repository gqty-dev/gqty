{
  "name": "@gqty/cli",
  "private": false,
  "sideEffects": false,
  "version": "4.0.2",
  "homepage": "https://gqty.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/gqty-dev/gqty.git"
  },
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "maintainers": [
    "Vicary Archangel <vicary.archangel@member.mensa.org>"
  ],
  "bin": {
    "gqty": "bin/gqty.mjs"
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "directory": "dist"
  },
  "keywords": [
    "client",
    "codegen",
    "gqty",
    "graphql",
    "interactive",
    "reactive"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "bob-tsm build.ts",
    "prepare": "bob-tsm build.ts",
    "postpublish": "gh-release",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch-coverage": "rimraf coverage && mkdir -p coverage/lcov-report && concurrently --raw \"jest --watchAll\" \"serve -l 8787 coverage/lcov-report\" \"wait-on tcp:8787 coverage/lcov-report/index.html && open-cli http://localhost:8787\""
  },
  "dependencies": {
    "@graphql-codegen/core": "^4.0.2",
    "@graphql-codegen/typescript": "^4.0.7",
    "@graphql-tools/delegate": "^10.0.11",
    "@graphql-tools/utils": "^10.2.2",
    "@graphql-tools/wrap": "^10.0.5",
    "@inquirer/prompts": "^5.0.5",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "cosmiconfig": "^9.0.0",
    "cross-fetch": "^4.0.0",
    "fast-glob": "^3.3.2",
    "gqty": "workspace:^",
    "graphql": "^16.8.2",
    "lodash-es": "^4.17.21",
    "micromatch": "^4.0.7",
    "prettier": "^2.8.8",
    "trading-signals": "^5.0.4"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/micromatch": "^4.0.7",
    "@types/node": "^20.14.5",
    "@types/prettier": "^2.7.3",
    "bob-tsm": "^1.1.2",
    "esbuild": "^0.21.5",
    "open-cli": "^8.0.0",
    "p-lazy": "^3.1.0",
    "rimraf": "^5.0.7",
    "serve": "^14.2.3",
    "size-limit": "^11.1.4",
    "test-utils": "workspace:^",
    "tmp-promise": "^3.0.3",
    "wait-on": "^7.2.0"
  }
}
