    "@types/node-schedule": "^1.3.2",
    "tsup": "^4.13.1",
    "typescript": "^4.3.5"
  },
  "tsup": {
    "format": [
      "esm"
    ],
    "target": "es2020",
    "clean": true,
    "entryPoints": [
      "src/index.ts"
    ],
    "skipNodeModulesBundle": true,
    "splitting": false,
    "silent": true
  }
