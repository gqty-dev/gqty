    "@graphql-ez/plugin-websockets": "^0.6.1",
    "@prisma/client": "^2.28.0",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "fastify": "^3.20.1",
    "fastify-cors": "^6.0.2",
    "graphql": "^15.5.1",
