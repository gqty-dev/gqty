  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsup",
    "db:prepare": "prisma generate",
    "dev": "tsup --watch --onSuccess \"node dist/index.js\"",
    "start": "node dist/index.js",
    "typecheck": "prisma generate && tsc"
  },
  "dependencies": {
    "@graphql-ez/fastify": "^0.9.3",
    "@graphql-ez/plugin-altair": "^0.7.1",
    "@graphql-ez/plugin-codegen": "^0.7.0",
    "@graphql-ez/plugin-modules": "^0.8.0",
    "@graphql-ez/plugin-scalars": "^0.7.1",
    "@graphql-ez/plugin-voyager": "^0.7.1",
    "@graphql-ez/plugin-websockets": "^0.9.3",
    "@prisma/client": "^2.30.3",
    "date-fns": "^2.23.0",
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "fastify": "^3.20.2",
    "fastify-cors": "^6.0.2",
    "graphql": "^15.5.2",
    "graphql-ez": "^0.12.7",
    "graphql-scalars": "^1.10.0",
    "graphql-voyager": "^1.0.0-rc.31",
    "jsonwebtoken": "^8.5.1",
    "lodash-es": "^4.17.21",
    "ms": "^2.1.3",
    "node-json-db": "^1.3.0",
    "node-schedule": "^2.0.0",
    "p-lazy": "^4.0.0",
    "prisma": "^2.30.3"
  },
  "devDependencies": {
    "@types/faker": "^5.5.8",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.172",
    "@types/lodash-es": "^4.17.4",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.7.10",
    "@types/node-schedule": "^1.3.2",
    "tsup": "^4.14.0",
    "typescript": "^4.4.2"
  },
  "tsup": {
    "format": [
      "esm"
    ],
    "target": "es2020",
    "clean": true,
    "entryPoints": [
      "src/index.ts"
    ],
    "skipNodeModulesBundle": true,
    "splitting": false,
    "silent": true
  }
}
