  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    },
    "./jest.config.js": "./jest.config.js"
  },
  "bin": {
    "jest-test": "bin/jest-test.js"
  },
  "scripts": {
    "build": "bob-ts -i src -f interop",
    "prepare": "pnpm build",
    "start": "nodemon --exec \"concurrently pnpm:build tsc\" -w src/index.ts",
    "test": "jest --config local.jest.config.js"
  },
  "dependencies": {
    "@graphql-ez/fastify": "^0.12.0",
    "@graphql-ez/fastify-testing": "^0.4.0",
    "@graphql-ez/plugin-codegen": "^0.8.0",
    "@graphql-ez/plugin-schema": "^0.9.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@types/jest": "^29.2.5",
    "cross-env": "^7.0.3",
    "fastify": "^4.11.0",
    "graphql": "^16.6.0",
    "graphql-ez": "^0.16.0",
    "jest": "^29.3.1",
    "jest-watch-typeahead": "^2.2.1",
    "prettier": "^2.8.2",
    "randomstring": "^1.2.3",
    "ts-jest": "^29.0.5",
    "wait-for-expect": "^3.0.2"
  },
  "devDependencies": {
    "@graphql-ez/plugin-websockets": "^0.11.0",
    "@jest/types": "^29.3.1",
    "@types/node": "^18.11.18",
    "@types/randomstring": "^1.1.8",
    "bob-esbuild-cli": "^4.0.0",
    "bob-ts": "^4.1.1",
    "concurrently": "^7.6.0",
    "esbuild": "^0.16.17",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
