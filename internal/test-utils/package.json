{
  "name": "test-utils",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": {
      "require": "./dist/*.js",
      "import": "./dist/*.mjs"
    },
    "./jest.config.js": "./jest.config.js"
  },
  "bin": {
    "jest-test": "bin/jest-test.js"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "bob-ts -i src -f interop",
    "prepare": "pnpm build",
    "size": "size-limit",
    "start": "nodemon --exec \"concurrently pnpm:build tsc\" -w src/index.ts",
    "test": "jest --config local.jest.config.js"
  },
  "dependencies": {
    "@graphql-ez/fastify": "^0.9.4",
    "@graphql-ez/fastify-testing": "^0.1.3",
    "@graphql-ez/plugin-codegen": "^0.7.6",
    "@graphql-ez/plugin-schema": "^0.8.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/jest": "^27.0.3",
    "cross-env": "^7.0.3",
    "esbuild": "^0.13.14",
    "fastify": "^3.24.0",
    "graphql": "^16.0.1",
    "graphql-ez": "^0.13.5",
    "jest": "^27.3.1",
    "jest-watch-typeahead": "^1.0.0",
    "prettier": "^2.4.1",
    "randomstring": "^1.2.1",
    "ts-jest": "^27.0.7",
    "wait-for-expect": "^3.0.2"
  },
  "devDependencies": {
    "@graphql-ez/plugin-websockets": "^0.10.2",
    "@jest/types": "^27.2.5",
    "@size-limit/preset-small-lib": "^7.0.0",
    "@types/randomstring": "^1.1.8",
    "bob-esbuild-cli": "^3.0.1",
    "bob-ts": "^1.2.2",
    "concurrently": "^6.4.0",
    "size-limit": "^7.0.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "size-limit": [
    {
      "path": "dist/test-utils.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/test-utils.esm.js",
      "limit": "10 KB"
    }
  ]
}
