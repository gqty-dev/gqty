{
  "name": "test-utils",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "author": "PabloSzx <pablosaez1995@gmail.com>",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "src/index.d.ts",
  "bin": {
    "jest-test": "bin/jest-test.js"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "rimraf dist && tsup src/index.ts --format esm,cjs",
    "prepare": "pnpm build",
    "size": "size-limit",
    "start": "nodemon --exec \"concurrently pnpm:build tsc\" -w src/index.ts",
    "test": "jest --config local.jest.config.js"
  },
  "dependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@types/jest": "^27.0.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.12.26",
    "fastify": "^3.21.0",
    "get-port": "^5.1.1",
    "graphql": "^15.5.3",
    "jest": "^27.1.1",
    "jest-watch-typeahead": "^0.6.4",
    "mercurius": "^8.2.1",
    "mercurius-codegen": "^2.0.0",
    "mercurius-integration-testing": "^3.2.0",
    "prettier": "^2.3.2",
    "randomstring": "^1.2.1",
    "ts-jest": "^27.0.5",
    "wait-for-expect": "^3.0.2"
  },
  "devDependencies": {
    "@jest/types": "^27.1.1",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/randomstring": "^1.1.7",
    "bob-esbuild-cli": "^1.0.1",
    "concurrently": "^6.2.1",
    "es-jest": "^1.3.0",
    "rimraf": "^3.0.2",
    "size-limit": "^5.0.3",
    "tslib": "^2.3.1",
    "tsup": "^4.14.0",
    "typescript": "^4.4.2"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/test-utils.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/test-utils.esm.js",
      "limit": "10 KB"
    }
  ]
}
