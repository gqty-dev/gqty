    "size": "size-limit",
    "start": "bob-esbuild watch",
    "test": "jest",
    "test:watch": "bob-esbuild watch"
  },
  "dependencies": {
    "@gqty/cli": "workspace:^1.1.4",
    "@graphql-typed-document-node/core": "^3.1.0",
    "fastify": "^3.20.2",
    "gqty": "workspace:^1.1.2",
    "graphql": "^15.5.2",
    "lodash": "^4.17.21",
    "mercurius": "^8.1.3",
    "mercurius-codegen": "^2.0.0",
    "randomstring": "^1.2.1",
    "test-utils": "workspace:^0.1.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^5.0.3",
    "@types/lodash": "^4.14.172",
    "@types/randomstring": "^1.1.7",
    "bob-esbuild-cli": "^1.0.1",
    "jest": "^27.1.0",
    "mercurius-integration-testing": "^3.2.0",
    "size-limit": "^5.0.3",
